{
  "expo": {
    "name": "Koch Assistent",
    "slug": "koch-assistent",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "kochapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kochapp.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-router",
      [
        "expo-in-app-purchases",
        {
          "appleTeamId": "U3H5S3GC83"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}