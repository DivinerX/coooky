{
  "expo": {
    "name": "Koch Assistent",
    "slug": "koch-assistent",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "kochapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kochapp.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-router",
      "react-native-iap"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "de130265-b633-4df1-bc97-1164b451ef8f"
      }
    },
    "owner": "spgdev"
  }
}
